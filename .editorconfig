# EditorConfig https://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8
indent_size = 4
indent_style = space
max_line_length = 120
tab_width = 4
trim_trailing_whitespace = true

[*.java]
indent_style = tab
